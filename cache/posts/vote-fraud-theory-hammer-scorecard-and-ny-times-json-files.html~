<span class="mf">49718.495999999344</span> <span class="n">Trump</span> <span class="n">to</span> <span class="n">Biden</span>
<span class="mf">39051.33700000135</span> <span class="n">Trump</span> <span class="n">to</span> <span class="n">Third</span>
<span class="mf">27413.24899999972</span> <span class="n">BIDEN</span> <span class="n">LOST</span>
<span class="mf">7159.232000000076</span> <span class="n">Biden</span> <span class="n">to</span> <span class="n">Trump</span>
<span class="mf">20254.016999999643</span> <span class="n">Biden</span> <span class="n">to</span> <span class="n">Third</span>
<span class="mf">223140.98500000124</span> <span class="mi">3</span><span class="n">RD</span> <span class="n">PARTY</span> <span class="n">LOST</span>
